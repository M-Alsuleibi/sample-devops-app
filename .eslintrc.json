{
  "env": {
    "es2022": true,
    "node": true,
    "jest": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["warn"],
    "comma-dangle": ["error", "never"],
    "semi": ["error", "always"]
  },
  "ignorePatterns": ["/assets/**"]
}


